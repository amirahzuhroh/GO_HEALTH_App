<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projectakhirlagi">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ResultActivity" />
        <activity
            android:name=".CalculatorActivity"
            android:label="@string/title_activity_calculator"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ProfileActivity"
            android:label="@string/title_activity_profile"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".JournalActivity"
            android:label="@string/title_activity_journal"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ActActivity"
            android:label="@string/title_activity_act"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TargetActivity"
            android:label="@string/title_activity_target"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TodayActivity"
            android:label="@string/title_activity_today"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>